{
  "name": "derralf/silverstripe-minigallery",
  "description": "add a mini gallery to any page type",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "mini gallery"
  ],
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "symbiote/silverstripe-gridfieldextensions": "^3.1",
    "derralf/silverstripe-gridfieldtogglevisibility": "^1.0",
    "colymba/gridfield-bulk-editing-tools": "^3.0",
    "jonom/focuspoint": "^3.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "expose": [
      "client/dist"
    ]
  },
  "autoload": {
    "psr-4": {
      "Derralf\\Minigallery\\": "src/"
    }
  }

}
